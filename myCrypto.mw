local x,ekey,i,j,final,d,finalkey;
if text = &quot;&quot; then
    error &quot;Invalid Input: Input can not be empty&quot;;
end if;
final := [];
x := encode(text);
ekey := encode(key);
finalkey := ekey;
if nops(x) &lt;&gt; nops(finalkey) then
    do
        finalkey := [ op(finalkey), op(ekey)];
    until ( nops(x) &lt;= nops(finalkey)):
end if;
d := substring(direction,1);
if d = &quot;d&quot; or d = &quot;D&quot; then
    for j from 1 to nops(x) do
        final := [ op(final), irem((op(j,x)-op(j,finalkey)+26),26)];
    end do:
    final := decode(final, &quot;l&quot;);
else
    for j from 1 to nops(x) do
        final := [ op(final), irem(op(j,x) + op(j,finalkey),26)];
    end do:
    final := decode(final, &quot;u&quot;);
end if;
return final;
end proc:</Text-field>
