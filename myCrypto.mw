<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">hill := proc(text, key, direction := &quot;e&quot;)
local x,dim,i,j,final,d,xmat,ikey;
if text = &quot;&quot; then
    error &quot;Invalid Input: Input can not be empty&quot;;
end if;
if gcd(26, Det(key)mod 26) &gt; 1 then
    error &quot;Invalid key matrix: No inverse exists mod 26&quot;
end if:
final := [];
x := encode(text);
dim := ColumnDimension(key);
if irem(nops(x),dim)&lt;&gt; 0 then
    do
        x := [ op(x), op(encode(&quot;z&quot;))];
    until (modp(nops(x),dim) = 0):
end if;
d := substring(direction,1);
if d = &quot;d&quot; or d = &quot;D&quot; then
    ikey := Mod(26,MatrixInverse(key),integer[]);
    for j from 1 to nops(x) do
        xmat := Matrix(dim,1,[op(j..(j+dim-1),x)]);
        final := [ op(final), op(map(irem,convert(ikey.xmat, list),26))];
        j += dim-1;
    end do:
    final := decode(final, &quot;l&quot;);
else
    for j from 1 to nops(x) do
        xmat := Matrix(dim,1,[op(j..(j+dim-1),x)]);
        final := [ op(final), op(map(irem,convert(key.xmat, list),26))];
        j += dim-1;
    end do:
    final := decode(final, &quot;u&quot;);
end if;
return final;
end proc:</Text-field>
</Input>
