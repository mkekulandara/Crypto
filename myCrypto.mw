<Group labelreference="L174" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">scytale := proc(text,key :: integer,direction := &quot;e&quot;)
local x,row,final,n,m,i,ltr,d;
final := [];
if text = &quot;&quot; then
    error &quot;Invalid Input: Input can not be empty&quot;;
end if;
x := encode(text);
d := substring(direction,1);
if irem(nops(x),key) != 0 then
    row := iquo(nops(x),key) + 1;
else
    row := iquo(nops(x),key);
end if;
if d = &quot;d&quot; or d = &quot;D&quot; then
for m from 1 by 1 to key do
    i := m;
    for n from 1 by 1 to row do
        if i &gt; nops(x) then
            ltr := op(encode(&quot;z&quot;));
        else
            ltr := op(i,x);
        end if;
        final := [op(final),ltr];
        i += key; 
    end do;
end do;
final := decode(final, &quot;l&quot;);
else
for m from 1 by 1 to row+1 do
    i := m;
    for n from 1 by 1 to key do
        if i &gt; nops(x) then
            ltr := op(encode(&quot;z&quot;));
        else
            ltr := op(i,x);
        end if;
        final := [op(final),ltr];
        i += row+1; 
    end do;
end do;
final := decode(final, &quot;U&quot;);
end if;
return final;
end proc:</Text-field>
</Input>
