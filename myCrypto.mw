<Group hide-input="false" labelreference="L195" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">decode := proc(codebytes :: list, case)
local x, c;
c := substring(case,1);
if c = &quot;U&quot; or c = &quot;u&quot; then
    x := map(`+`,codebytes, 65);
elif c = &quot;L&quot; or c = &quot;l&quot; then
    x := map(`+`,codebytes, 97);
else
    error &quot;Invalid Case : Case needs to be UPPER or lower&quot;;
end if;
x := convert(x, bytes);
if x = &quot;&quot; then
    error &quot;Invalid Input : Input can not be decoded&quot;;
end if;
return x;
end proc:</Text-field>
</Input>
</Group>
