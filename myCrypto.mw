<Group labelreference="L191" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">shift := proc(text, key::integer, direction := &quot;e&quot;)
local x,d,final;
if text = &quot;&quot; then
    error &quot;Invalid Input: Input can not be empty&quot;;
end if;
x := encode(text);
d := substring(direction,1);
if d = &quot;d&quot; or d = &quot;D&quot; then
    x := map(modp, map( `-`, x, key), 26);
    final := decode(x, &quot;l&quot;);
else
    x := map(modp, map( `+`, x, key), 26);
    final := decode(x, &quot;U&quot;);
end if;
return final;
end proc:</Text-field>
</Input>
</Group>
