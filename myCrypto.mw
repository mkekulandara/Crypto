<Group labelreference="L193" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">affine := proc(text, key :: list, direction := &quot;e&quot;)
local x,final,inverse,d;
if text = &quot;&quot; then
    error &quot;Invalid Input: Input can not be empty&quot;;
end if;
x := encode(text);
d := substring(direction,1);
if gcd(26, op(1,key)) &gt; 1 then
         error &quot;Invalid key: No inverse exists&quot;;
end if;
if d = &quot;d&quot; or d = &quot;D&quot; then    
    inverse := 1/op(1,key) mod 26;
    x := map(modp, map( `-`, map( `*`, x, inverse) ,op(2,key)*inverse), 26);
    final := decode(x, &quot;l&quot;);
else
    x := map(modp, map( `+`, map( `*`, x, op(1,key)) ,op(2,key)), 26);
    final := decode(x, &quot;U&quot;);
end if;
return final;
