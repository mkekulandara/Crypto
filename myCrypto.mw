<Group hide-input="false" labelreference="L196" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">encode := proc(text)
local x,m,final;
final := [];
x := convert(text,bytes);
for m from 1 by 1 to nops(x) do
    if op(convert(A,bytes))&lt;= op(m,x) and op(m,x) &lt;= op(convert(Z,bytes)) then
        final := [op(final) , op(m,x)-65 ];
    elif op(convert(a,bytes))&lt;= op(m,x) and op(m,x) &lt;= op(convert(z,bytes)) then
        final := [op(final) , op(m,x)-97 ];
    end if;
end do;
if final = [] then
    error &quot;Invalid Input : Input can not be encoded&quot;
end if;
return final;    
end proc:</Text-field>
</Input>
</Group>
