<Input><Text-field prompt="&gt; " style="Maple Input" spaceabove="0" linebreak="space" rightmargin="0" linespacing="0.0" initial="0" firstindent="0" leftmargin="0" bulletsuffix="" alignment="left" bullet="none" spacebelow="0" pagebreak-before="false">autokey := proc(text, key, direction := &quot;e&quot;)
local x,ekey,i,j,final,d;
if text = &quot;&quot; then
    error &quot;Invalid Input: Input can not be empty&quot;;
end if;
final := [];
x := encode(text);
ekey := encode(key);
d := substring(direction,1);
if d = &quot;d&quot; or d = &quot;D&quot; then
    for j from 1 to nops(x) do
        final := [ op(final), irem((op(j,x)-op(j,ekey)+26),26)];
        ekey := [op(ekey), irem((op(j,x)-op(j,ekey)+26),26)];
    end do:
    final := decode(final, &quot;l&quot;);
else
    for i from 1 to nops(x)-1 do
        ekey := [op(ekey), op(i,x)]; 
    end do:
    for j from 1 to nops(x) do
        final := [ op(final), irem(op(j,x) + op(j,ekey),26)];
    end do:
    final := decode(final, &quot;u&quot;);
end if;
return final;
end proc:</Text-field>
