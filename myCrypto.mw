<Group labelreference="L214" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" spaceabove="0" linebreak="space" rightmargin="0" linespacing="0.0" initial="0" firstindent="0" leftmargin="0" bulletsuffix="" alignment="left" bullet="none" spacebelow="0" pagebreak-before="false">permutationCipher := proc(text, key, direction := &quot;e&quot;)
local x,i,d,final,cipher,keylngth,j,index;
if text = &quot;&quot; then
    error &quot;Invalid Input: Input can not be empty&quot;;
end if;
final := [];
x := encode(text);
keylngth := nops(key);
d := substring(direction,1);
if d = &quot;d&quot; or d = &quot;D&quot; then
    for i from 0 to nops(x)-1 by 1 do
         for j from 1 to keylngth by 1 do
              index := i + Search(j, key);
              final := [op(final), op(index, x)];
         end do:
         i += keylngth-1;
    end do:
    final := decode(final, &quot;l&quot;);
else
    if irem(nops(x),keylngth) &gt; 0 then
        do
            x := [ op(x), op(encode(&quot;z&quot;))];
        until (modp(nops(x),keylngth) = 0):
    end if;
    for i from 0 to nops(x)-1 by 1 do
         for j from 1 to keylngth by 1 do
              final := [op(final), op((i + op(j,key)), x)];
         end do:
         i += keylngth-1;
    end do:
    final := decode(final, &quot;u&quot;);
end if;
return final;
end proc:</Text-field>
</Input>
